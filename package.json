{
  "name": "essential-cu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js -p $PORT",
    "build": "next build",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "capture": "node data/impact-reports/onward/capture-data.js",
    "styles:onward": "node-sass ./styles/impact-report/onward/styles.scss ./public/impact-reports/onward/styles.css --output-style compressed",
    "styles:2021": "tailwindcss -i ./styles/impact-report/2021/global.css  -o ./public/impact-reports/2021/tailwind.css",
    "styles:2022": "NODE_ENV=production tailwindcss -i ./styles/impact-report/2021/global.css  -o ./public/impact-reports/2021/tailwind.css --minify",
    "test": "lint && cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/integration/**/*'",
    "lint": "eslint ."
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.2.0",
    "@nivo/core": "^0.73.0",
    "@nivo/pie": "^0.73.0",
    "cloudinary-core": "^2.11.4",
    "contentful": "^8.4.2",
    "express": "^4.17.1",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-player": "^2.9.0",
    "react-responsive": "^8.2.0",
    "react-share": "^4.4.0",
    "styled-jsx": "^4.0.1"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "@types/node": "^16.4.12",
    "@types/react": "^17.0.19",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  }
}
