{
  "name": "essential-cu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js -p $PORT",
    "build": "next build",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "capture": "node data/impact-reports/onward/capture-data.js",
    "styles:onward": "node-sass ./styles/impact-report/onward/styles.scss ./public/impact-reports/onward/styles.css --output-style compressed",
    "styles:2021": "tailwindcss -w -i ./styles/impact-report/joy/global.css  -o ./public/impact-reports/joy/tailwind.css",
    "styles:2022": "NODE_ENV=production tailwindcss -i ./styles/impact-report/joy/global.css  -o ./public/impact-reports/joy/tailwind.css --minify",
    "test": "lint && cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/integration/**/*'",
    "lint:2021": "eslint components/impact-reports/joy/* && eslint pages/impact-reports/joy/*"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@headlessui/react": "^1.4.1",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "cloudinary-core": "^2.12.0",
    "contentful": "^9.1.3",
    "express": "^4.17.1",
    "next": "11.1.3",
    "react": "17.0.2",
    "react-circular-progressbar": "^2.0.4",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-player": "^2.9.0",
    "react-responsive": "^8.2.0",
    "react-share": "^4.4.0",
    "recharts": "^2.1.5",
    "styled-jsx": "^4.0.1",
    "usehooks-ts": "^2.1.1"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.3.7",
    "babel-eslint": "^10.1.0",
    "cypress": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "prop-types": "^15.7.2",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4"
  }
}
